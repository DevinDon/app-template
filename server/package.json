{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "description": "The server end of this app.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "scripts": {
    "build": "tsc",
    "debug": "node -r ts-node/register --inspect-brk src/main/index.ts",
    "dist": "node dist",
    "start": "npm run build && npm run dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register"
  },
  "keywords": [
    "server",
    "nodejs",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@iinfinity/rester": "^0.7.1",
    "@iinfinity/logger": "^0.2.0",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/node": "^11.12.1",
    "jasmine": "^3.3.1",
    "jasmine-console-reporter": "^3.1.0",
    "ts-node": "^8.0.3"
  }
}
